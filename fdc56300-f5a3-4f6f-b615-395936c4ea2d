				<script type="text/javascript">
					(function($) {
						$('#edition_nav, #region_nav')
							.click(function(){
								$(this).toggleClass('edition_nav_selected');
							})
							.mouseleave(function(){
								var e = $(this),
									t = e.data('timer');
								t && clearTimeout(t);
								t = setTimeout(function(){
									e.hasClass('edition_nav_selected') && e.removeClass('edition_nav_selected')
								}, 1000);
								e.data('timer', t);
							})
							.mouseenter(function(){
								var t = $(this).data('timer');
								t && clearTimeout(t);
							});
					})(jQuery);
				</script>
           
